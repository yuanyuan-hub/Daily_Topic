import java.util.*;
public class Solution {
    public double Power(double base, int exponent) {
        if(exponent < 0) {
            base = 1/base;
            exponent = -exponent;
        }
        double res = 1.0;
        for(int i = 0; i < exponent;i++) {
            res = res*base;
        }
        return res;
  }
}
