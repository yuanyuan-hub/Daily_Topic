import java.util.*;


public class Solution {
    /**
     * return the min number
     * @param arr int整型一维数组 the array
     * @return int整型
     */
    public int minNumberdisappered (int[] arr) {
        // write code here
        int tem = 1;
        int index = 0;
        for(int i = 0;i < arr.length;i++){
            if(arr[i] == tem){
                tem++;
                index = 0;
            }
            index++;
        }
        
        return tem;
    }
}
